/* 图片 */

/* uploader */

page {
  font-size: 11pt;
  background: #f5f5f5;
}

.domes_title {
  font-size: 20pt;
  color: #09bb07;
  padding: 70rpx 0;
  margin: 0 15%;
  text-align: center;
  font-weight: 400;
}

.ui_flex {
  position: relative;
}

.ui_flex:before {
  content: '';
  height: 2rpx;
  background: #eee;
  clear: both;
  display: block;
}

.ui_flex_item {
  position: relative;
  width: 33.3%;
  height: 250rpx;
  float: left;
  border-right: 2rpx solid #eee;
  border-bottom: 2rpx solid #eee;
  text-align: center;
  box-sizing: border-box;
}

.ui_flex_content {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 100%;
  height: 100rpx;
}

.ui_flex_content image {
  width: 56rpx;
  height: 56rpx;
  margin: 0 auto;
}

.ui_flex_content view {
  height: 44rpx;
}

/* button */

.ui_btn {
  font-size: 13pt;
  height: 90rpx;
  line-height: 90rpx;
  width: 85%;
  text-align: center;
  border-radius: 15rpx;
  margin: 20rpx auto;
}

.ui_btn_default {
  border: 2rpx solid #d9d9d9;
  background: #fff;
}

.ui_btn_primary {
  border: 2rpx solid #09bb07;
  background: #09bb07;
  color: #fff;
}

.ui_btn_warn {
  border: 2rpx solid #e64340;
  background: #e64340;
  color: #fff;
}

.ui-btn_disabled {
  opacity: 0.3;
}

/* cell */

.ui_cell {
  display: flex;
  position: relative;
  height: 100rpx;
  line-height: 60rpx;
  padding: 20rpx 0rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
  transform: translate3d(0rpx, 0px, 0px);
}

.ui_cell_hd {
  width: 40rpx;
  height: 40rpx;
  margin-top: 10rpx;
  margin-right: 10rpx;
}

.ui_cell_hd image {
  width: 100%;
  height: 100%;
}

.ui_cell_bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 26rpx;
}

.ui_cell_bd_must {
  position: relative;
}

.ui_cell_bd_must:after {
  display: inline;
  content: '*';
  color: #e62b2b;
}

.ui_cell_ft {
  color: #999;
  text-align: right;
}

.ui_cell_input {
  height: 60rpx;
  line-height: 60rpx;
  flex: 3;
  padding-left: 30rpx;
  font-size: 26rpx;
}

.ui_cell_getCode {
  z-index: 999;
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  height: 60rpx;
  line-height: 56rpx;
  padding: 0 20rpx;
  font-size: 9pt;
  color: #f4a83a;
  border: 2rpx solid #f4a83a;
  border-radius: 35rpx;
  box-sizing: border-box;
}

.ui_cell_aleryCode {
  z-index: 999;
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  height: 60rpx;
  line-height: 56rpx;
  padding: 0 20rpx;
  font-size: 9pt;
  color: #d9d9d9;
  border: 2rpx solid #d9d9d9;
  border-radius: 35rpx;
  box-sizing: border-box;
}

.ui_cell_picker {
  height: 60rpx;
  line-height: 60rpx;
  flex: 3;
}

.ui_cell_switch {
  position: relative;
  top: -4rpx;
}

.ui_cell_right {
  width: 20rpx;
  height: 20rpx;
  margin: 20rpx 0 0 20rpx;
  box-sizing: border-box;
  border-top: 2rpx solid #d9d9d9;
  border-right: 2rpx solid #d9d9d9;
  transform: rotate(45deg);
}

.ui_cell_del {
  position: absolute;
  height: 100%;
  line-height: 100rpx;
  width: 120rpx;
  text-align: center;
  background: red;
  right: -120rpx;
  top: 0;
  color: #fff;
}

.ui_cell_cancel {
  position: absolute;
  height: 100%;
  line-height: 100rpx;
  width: 120rpx;
  text-align: center;
  background: #d9d9d9;
  right: -240rpx;
  top: 0;
  color: #fff;
}

.ui_cell_toLeft {
  animation: fnToLeft 0.5s forwards;
}

.ui_cell_toRight {
  animation: fnToRight 0.5s forwards;
}

@keyframes fnToLeft {
  from {
    transform: translate3d(0rpx, 0px, 0px);
  }

  to {
    transform: translate3d(-240rpx, 0px, 0px);
  }
}

@keyframes fnToRight {
  from {
    transform: translate3d(-240rpx, 0px, 0px);
  }

  to {
    transform: translate3d(0rpx, 0px, 0px);
  }
}

.ui_panel {
  position: relative;
  padding: 30rpx;
  display: flex;
  border-bottom: 2rpx solid #eee;
}

.ui_panel_hd {
  margin-right: 20rpx;
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
}

.ui_panel_hd image {
  width: 100%;
  background: #eee;
  max-height: 100%;
  vertical-align: top;
}

.ui_panel_bd {
  flex: 1;
}

/* form */

textarea {
  position: relative;
  padding: 20rpx 30rpx;
  width: 100%;
  margin: 40rpx 0;
  box-sizing: border-box;
  border-top: 2rpx solid #eee;
  border-bottom: 2rpx solid #eee;
}

textarea text {
  position: absolute;
  right: 40rpx;
  bottom: 40rpx;
  color: #d9d9d9;
}

.ui_agree {
  display: flex;
  font-size: 9pt;
  height: 70rpx;
  line-height: 50rpx;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
}

.ui_agree_text {
  line-height: 60rpx;
}

.ui_agree_text text {
  color: #586c94;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  background: #fff;
  box-sizing: border-box;
}

/* checkbox选中时样式 */

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  background: #586c94;
  color: #fff;
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border: none;
}

/* pages/dialog/dialog.wxss */

.ui_mask {
  position: fixed;
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
  background: #000;
  -webkit-transition-duration: 0.3s;
  transition-duration: 0.3s;
  visibility: hidden;
}

.ui_mask_visible {
  opacity: 0.5;
  visibility: visible;
}

.ui_dialog {
  z-index: 999;
  opacity: 0;
  width: 80%;
  max-width: 600rpx;
  position: fixed;
  top: 45%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: #fff;
  border-radius: 6rpx;
  visibility: hidden;
  transition-duration: 0.1s;
  overflow: hidden;
  text-align: center;
}

.ui_dialog_visible {
  opacity: 1;
  visibility: visible;
}

.ui_dialog_hd {
  padding: 40rpx 30rpx 10rpx 30rpx;
}

.ui_dialog_bd {
  padding: 20rpx 30rpx 20rpx 30rpx;
  min-height: 80rpx;
  line-height: 1.3;
  word-wrap: break-word;
  word-break: break-all;
  color: #999;
}

.ui_dialog_input {
  padding: 8rpx 12rpx;
  border: 2rpx solid #ccc;
  box-sizing: border-box;
  height: 70rpx;
  width: 80%;
  margin: 0 auto;
  margin-top: 20rpx;
}

.ui_dialog_ft {
  position: relative;
  line-height: 96rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-top: 2rpx solid #d5d5d6;
}

.ui_dialog_cancel {
  flex: 1;
  font-size: 13pt;
  color: #5f646e;
}

.ui_dialog_sure {
  flex: 1;
  font-size: 13pt;
  color: #5677fc;
  border-left: 2rpx solid #d5d5d6;
}

.ui_dialog_title {
  font-size: 14pt;
  font-weight: 400;
}

.ui_nomore {
  position: relative;
  width: 65%;
  margin: 0 auto;
  border-top: 2rpx solid #e5e5e5;
  text-align: center;
  margin-top: 40rpx;
  height: 60rpx;
}

.ui_nomore_content {
  position: relative;
  top: -30rpx;
  padding: 0 15rpx;
  background-color: #fff;
  color: #999;
  display: inline-block;
  vertical-align: middle;
}

.ui_nomore_bit {
  position: absolute;
  top: -10rpx;
  left: 50%;
  margin-left: -13rpx;
  border: 8rpx solid #fff;
  background-color: #999;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
}

/* uploader */

.ui_uploader_cell {
  width: 93%;
  padding-left: 8px;
  box-sizing: border-box;
}

.ui_uploader_item {
  float: left;
  position: relative;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  width: 165rpx;
  height: 165rpx;
}

.ui_uploader {
  float: left;
  position: relative;
  margin-right: 25rpx;
  margin-bottom: 25rpx;
  width: 128rpx;
  height: 128rpx;
  border: 2rpx dashed #d9d9d9;
  box-sizing: border-box;
  margin-left: 26rpx;
}

.ui_uploader:before {
  content: " ";
  position: absolute;
  width: 4rpx;
  height: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader:after {
  content: " ";
  position: absolute;
  height: 4rpx;
  width: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader_item_icon {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}

.ui_uploader_item image {
