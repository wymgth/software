<view class="big">
  <view class="page__bd">
    <form bindsubmit="submitForm" bindreset="formReset">
      <view class='ui_uploader_cell'>
        <view>
          <!-- 软件全称 -->
          <view class="weui-cell__bd">
            <view class="u_names" style="flex:0.6;">软件全称</view>
            <view class="u_input">
              <input name="software_all" class="weui-input" type="text" value="{{list.software_all}}" disabled />
            </view>
          </view>
          <!-- 版本号 -->
          <view class="weui-cell__bd">
            <view class="u_names" style="flex:0.6;">版本号</view>
            <view class="u_input">
              <input name="version" class="weui-input" type="text" value="{{list.version}}" disabled/>
            </view>
          </view>
          <!-- 发表状态 -->
          <view class="weui-cell__bd">
            <view class="u_names">发表状态</view>
            <view class="u_input">
              <input name="version" class="weui-input" type="text" value="{{list.deliver_status}}" disabled/>
            </view>
          </view>
          <!-- 已发表状态时 -->
          <view class="weui-cell__bd" style="display:{{list.deliver_status == '已发表' ? 'flex':'none'}}">
            <view class="u_names" style="flex:1.3">首次发表日期</view>
            <view class="u_input">
              <view class="section">
                <input name="version" class="weui-input" type="text" value="{{list.start_time}}" disabled/>
              </view>
            </view>
          </view>
          <view class="weui-cell__bd" style="display:{{list.deliver_status == '已发表' ? 'flex':'none'}}">
            <view class="u_names" style="flex:1.3">首次发表地点</view>
            <view class="u_input">
              <input name="version" class="weui-input" type="text" value="{{list.country}}" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd" style="display:{{list.deliver_status == '已发表' ? 'flex':'none'}}">
            <view class="u_names" style="flex:1.3">首次发表地点</view>
            <view class="u_input">
              <input name="city" class="weui-input" type="text" placeholder="请输入城市" value="{{list.city}}" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd" style="display:{{list.deliver_status == '未发表' ? 'flex':'none'}}">
            <view class="u_names" style="flex:1.6;">著作权申请人/公司</view>
            <view class="u_input">
              <input name="copy_name" class="weui-input" type="text" value="{{list.copy_name}}" placeholder="请填写信息" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd"  style="display:{{list.deliver_status == '未发表' ? 'flex':'none'}}">
            <view class="u_names" style="flex:1.6;">著作权人/公司证件号</view>
            <view class="u_input">
              <input name="copy_number" class="weui-input" type="text" value="{{list.copy_number}}" placeholder="请填写证件号" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd" style="display:{{list.deliver_status == '未发表' ? 'flex':'none'}}">
            <view class="u_names" style="flex:1.6;">详细地址(执照地址)</view>
            <view class="u_input">
              <input name="copy_address" class="weui-input" type="text" value="{{list.copy_address}}" placeholder="请填写地址" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd">
            <view class="u_names">邮政编号</view>
            <view class="u_input">
              <input name="post_code" class="weui-input" type="text" value="{{list.post_code}}" placeholder="请填写邮政编号" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd">
            <view class="u_names">联系人</view>
            <view class="u_input">
              <input name="link_people" class="weui-input" type="text" value="{{list.link_people}}" placeholder="请填写联系人" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd">
            <view class="u_names">电话号码</view>
            <view class="u_input">
              <input name="telephone" class="weui-input" type="text" value="{{list.telephone}}" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd">
            <view class="u_names">E-mail</view>
            <view class="u_input">
              <input name="email" class="weui-input" type="text" value="{{list.email}}" placeholder="请填写E-mail" disabled/>
            </view>
          </view>
          <view class="weui-cell__bd">
            <view class="u_names">手机号码</view>
            <view class="u_input">
              <input name="iphone" class="weui-input" type="text" value="{{list.iphone}}" placeholder="请填写手机号码" disabled />
            </view>
          </view>
          <view class="weui-cell__bd" style="border:none">
            <view class="u_names">传真号码</view>
            <view class="u_input">
              <input name="fax_number" class="weui-input" type="text" value="{{list.fax_number}}"  disabled/>
            </view>
          </view>
        </view>
      </view>
 
    </form>
  </view>

</view>